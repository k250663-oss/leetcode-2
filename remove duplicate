#include <stdio.h>
void rdup(){
    int temp;
    int num;
    printf("enter numbers you would like to enter:");
    scanf("%d" ,&num);
    int arr[num];
    for(int i=0 ; i < num ;i++){
    printf("enter array element#%d: " , i+1);
    scanf("%d" ,&arr[i]);
}
for(int i=0 ; i<num ; i++){
printf("%d " , arr[i]);
}
for(int i = 0; i < num- 1; i++){
    for(int j = 0; j< num -1 - i;j++){
        if(arr[j] >arr[j+1]){
            int temp=arr[j];
            arr[j]=arr[j+1];
            arr[j+1] = temp;}
            }
}
for(int i =0 ; i<num-1 ; i++){
    if(arr[i] == arr[i+1]){
        arr[i+1] = -1;                          //replaced _ with -1
    }
}
printf("\n\ntransformed array:");
    for(int i=0 ; i<num ; i++){
printf("%d " , arr[i]);
}
    
}

int main() {
    rdup();

    return 0;
}
